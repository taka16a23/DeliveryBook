<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="info.no_ip.taka16.deliverybook.DebugDataMakerActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="503dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/debug_1_area"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="create 1 area data" />

        <Button
            android:id="@+id/debug_2_area"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="create 2 area data" />

        <Button
            android:id="@+id/debug_3_area"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="create 3 area data" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
